<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sierra.isoft">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconito"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconito"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="org.sierra.isoft.TemarioActivity"
            android:label="@string/title_activity_temario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="org.sierra.isoft.ResultadoActivity" />
        <activity android:name="org.sierra.isoft.ExamenUnaPreguntaActivity" />
        <activity android:name="org.sierra.isoft.ExamenActivity" />
        <activity android:name="org.sierra.isoft.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>